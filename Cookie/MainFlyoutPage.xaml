        <local:SideMenuPage />
    </FlyoutPage.Flyout>

    <FlyoutPage.Detail>
        <NavigationPage x:Name="_navigationPage" Title="Saved Recepies!">
            <x:Arguments>
                <local:SearchRecepiesPage />
            </x:Arguments>
        </NavigationPage>
    </FlyoutPage.Detail>
    
</FlyoutPage>